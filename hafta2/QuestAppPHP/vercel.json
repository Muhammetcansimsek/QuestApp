{
  "version": 2,
  "builds": [
    { "src": "quizzer/*.php", "use": "@vercel/php" }
  ],
  "routes": [
    {
      "src": "^/$",
      "dest": "/quizzer/index.php"
    },
    {
      "src": "^/quizzer/(.*)$",
      "dest": "/quizzer/$1"
    },
    {
      "src": "^(?!/fetch.php)(?!/process.php)(.*)$",
      "dest": "/$1"
    }
  ]
}
